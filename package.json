{
  "name": "remnant-collectables",
  "version": "1.0.0",
  "private": true,
  "description": "remnant-collectables",
  "author": "Robin Kuiper",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{jsx,tsx,js,ts,json,yml,yaml,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:html": "eslint . --format html",
    "fix": "pnpm prettier && pnpm lint:fix"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.13.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-google-gtag": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-plugin-styled-components": "^6.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-modal": "^3.16.1",
    "react-spinners": "^0.13.8",
    "react-tooltip": "^5.19.0",
    "styled-components": "^6.0.5"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-helmet": "^6.1.6",
    "@types/react-modal": "^3.16.0",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-gitlab": "^4.0.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.6"
  },
  "nano-staged": {
    "*.{js,ts,jsx,tsx}": [
      "pnpm lint:fix"
    ],
    "*.{js,ts,jsx,tsx,json,yml,yaml,md}": [
      "pnpm prettier"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  }
}
