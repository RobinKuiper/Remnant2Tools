{
  "name": "remnant-collectables",
  "version": "1.0.0",
  "private": true,
  "description": "remnant-collectables",
  "author": "Robin Kuiper",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "start": "cross-env ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "build": "gatsby build",
    "build:ci:test": "pnpm lint && pnpm test:ci && pnpm build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{jsx,tsx,js,ts,json,yml,yaml,md}\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:html": "eslint ./src --format html",
    "fix": "pnpm prettier && pnpm lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.13.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-gdpr-cookies": "^2.0.9",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-plugin-styled-components": "^6.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-modal": "^3.16.1",
    "react-spinners": "^0.13.8",
    "react-tooltip": "^5.19.0",
    "styled-components": "^6.0.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.44.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-helmet": "^6.1.6",
    "@types/react-modal": "^3.16.0",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.9",
    "babel-jest": "^29.6.2",
    "babel-preset-gatsby": "^3.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-gitlab": "^4.0.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-junit": "^16.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.6"
  },
  "nano-staged": {
    "src/*.{js,ts,jsx,tsx}": [
      "pnpm lint:fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  }
}
